	<content><![CDATA[
vector<int> fact;
void factorial(int n, int p){
    fact.resize(n, 1);
    for (int i = 1; i < n; ++i)
    {
        fact[i] = (fact[i] * fact[i-1]) % p;
    }
}
]]></content>
